<?xml version='1.0'?><PageContent xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.w3schools.com"><config><x>0</x><y>0</y><bg_color>0x99ccff</bg_color><bg_image><x>0</x><y>0</y><url>http://www.flysforum.net/vicatfyp/release/1_0/img/flyswsv3.jpg</url><alpha>50</alpha></bg_image></config><data><TextFieldMC depth="1"><x>50</x><y>100</y><width>200</width><height>20</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0"><B>WELCOME TO FLABER</B></FONT></P></TEXTFORMAT></text></TextFieldMC><TextFieldMC depth="2"><x>50</x><y>120</y><width>375</width><height>100</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">FLABER (FLAsh web BuildER) - As it&apos;s name, it is a pure Flash-based application which allows any people making their web pages in Flash format.</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">Actually this is a Final Year Project for my university programme BSC Computer Science (City University of Hong Kong) which is supervised by Dr. Andy Chun, Hon Wai.</FONT></P></TEXTFORMAT></text></TextFieldMC><RectangleMC depth="3"><x>45</x><y>245</y><width>410</width><height>240</height><rotation>0</rotation><corner>0</corner><line_style>2|0x666666|100</line_style><fill_color>0xffffff</fill_color><alpha>9.765625</alpha></RectangleMC><TextFieldMC depth="4"><x>50</x><y>250</y><width>200</width><height>20</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0"><B>UPDATE HISTORY</B></FONT></P></TEXTFORMAT></text></TextFieldMC><TextFieldMC scroll_bar="true" depth="5"><x>50</x><y>270</y><width>375</width><height>200</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">Beta Release 1.0 (Mar 15)</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- This release have numerous changes which will probably takes serveral paragraphs to mention. So here, I just make it short.</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Menu Bar navigation</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Insertion and editing of all the elements</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Admin Login Control</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (Fix) Many minor bugs</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (Fix) Some logics which will increase performance</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">Prototype version 0.4.7 (Jan 27)</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Supports page background color and background image</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Supports text fields with text formats</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Supports images of JPEG, GIF, PNG formats</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Supports media of SWF format</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Supports internal Flash linkings</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Supports external HTTP linkings with new browser window</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Supports rectangles with different dimensions and colors</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (Fixed) Navigation menu design improved with better proformance and security</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">Prototype version 0.3.8 (Jan 16)</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Message boxes to show system messages</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (New) Message field supports icon and coloring styles</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">- (Fixed) Scroll bar bug for extreme scrollings</FONT></P></TEXTFORMAT></text></TextFieldMC><RectangleMC depth="6"><x>500</x><y>245</y><width>250</width><height>240</height><rotation>0</rotation><corner>0</corner><line_style>2|0x666666|100</line_style><fill_color>0xffffff</fill_color><alpha>8.984375</alpha></RectangleMC><TextFieldMC depth="7"><x>505</x><y>250</y><width>200</width><height>20</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0"><B>USEFUL LINKS</B></FONT></P></TEXTFORMAT></text></TextFieldMC><LinkMC depth="8"><x>505</x><y>270</y><type>1</type><url>http://www.cs.cityu.edu.hk/~hwchun/</url><TextFieldMC depth="0"><x>0</x><y>0</y><width>200</width><height>25</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">=&gt; Dr. Andy Chun&apos;s Web</FONT></P></TEXTFORMAT></text></TextFieldMC></LinkMC><LinkMC depth="9"><x>505</x><y>290</y><type>1</type><url>http://www.flysforum.net/vicatfyp</url><TextFieldMC depth="0"><x>0</x><y>0</y><width>200</width><height>25</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#FFFFFF" LETTERSPACING="0" KERNING="0">=&gt; Vicker&apos;s FYP Blog</FONT></P></TEX