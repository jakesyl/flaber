<?xml version='1.0'?><PageContent xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.w3schools.com"><config><x>0</x><y>0</y><bg_color>0xffffff</bg_color><bg_image><x>0</x><y>0</y><url>img/flyswsv3.jpg</url><alpha>50</alpha></bg_image></config><data><TextFieldMC depth="15"><x>598</x><y>483</y><width>170</width><height>36</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0">* Internet Explorer user may use &quot;<B>CTRL+SHIFT+E&quot;</B> instead.<FONT FACE="_sans" SIZE="12" COLOR="#0B333C" /></FONT></P></TEXTFORMAT></text></TextFieldMC><TextFieldMC depth="14"><x>595</x><y>411</y><width>157</width><height>47</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0">Press &quot;<B>CTRL+E</B>&quot; with password &quot;<B>flaber</B>&quot; to enter admin mode.<FONT COLOR="#0B333C" /></FONT></P></TEXTFORMAT></text></TextFieldMC><RectangleMC depth="13"><x>569</x><y>401</y><width>202</width><height>143</height><rotation>-4</rotation><corner>0</corner><line_style>1|0xFFCC00|100</line_style><fill_color>0xffff66</fill_color><alpha>100</alpha></RectangleMC><RectangleMC depth="10"><x>580</x><y>300</y><width>170</width><height>70</height><rotation>0</rotation><corner>0</corner><line_style>1|0x999999|100</line_style><fill_color>0xffffff</fill_color><alpha>100</alpha></RectangleMC><LinkMC depth="12"><x>600</x><y>345</y><type>1</type><url>http://www.sourceforge.net/projects/flaber</url><TextFieldMC depth="0"><x>0</x><y>0</y><width>150</width><height>16</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#003366" LETTERSPACING="0" KERNING="0"><B>FLABER@SourceForge</B></FONT></P></TEXTFORMAT></text></TextFieldMC></LinkMC><ImageMC depth="11"><x>620</x><y>310</y><width>88</width><height>31</height><rotation>0</rotation><url>img/sflogo.png</url></ImageMC><ImageMC depth="9"><x>580</x><y>90</y><width>172</width><height>203</height><rotation>0</rotation><url>img/logo_full.png</url></ImageMC><ImageMC depth="8"><x>50</x><y>245</y><width>24</width><height>28</height><rotation>0</rotation><url>img/logo_icon.png</url></ImageMC><ImageMC depth="7"><x>50</x><y>95</y><width>24</width><height>28</height><rotation>0</rotation><url>img/logo_icon.png</url></ImageMC><TextFieldMC depth="6"><x>50</x><y>130</y><width>499</width><height>89</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">FLABER (FLAsh web BuildER)</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">A pure Flash-based application which allows any people with any experience making their Flash web pages.</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">This is a university Final Year Project developed by Vicker LEUNG, Chi Yiu and supervised by Dr. Andy CHUN, Hon Wai.</FONT></P></TEXTFORMAT></text></TextFieldMC><TextFieldMC scroll_bar="true" depth="5"><x>50</x><y>280</y><width>475</width><height>200</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0"><B>Version 1.0 RC 2 (Mar 28)</B></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) page can be added through the menu <FONT COLOR="#FF9900">File &gt; New Page</FONT></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) page can be opened through the menu <FONT COLOR="#FF9900">File &gt; Open Page</FONT></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) page transition effects can be added using <FONT COLOR="#FF9900">Modify &gt; Web Properties</FONT></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) background color and image can be set using <FONT COLOR="#FF9900">Modify &gt; Page Properties</FONT></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) navigation, textfields, images, links and rectangles now supports delete</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) tooltips box with detailed button descriptions to make every function clearer</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) running in local pressing Ctrl+E will enter admin mode directly without login</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) admin password, page and image retrieval link back to parent directory</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) edit panel now only appear when mouse click</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) page items now only show &quot;hand cursor&quot; when it allows actions</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) improved saving mechanism on background image</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) improved scroll bar accuracy and reduced calculations</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) improved status bar interface with buttons for expansion</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) numerous spelling mistakes are corrected</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0"><B>Version 1.0 RC 1 (Mar 15)</B></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Menu Bar navigation</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Insertion and editing of all the elements</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Admin Login Control</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) Many minor bugs</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) Some logics which will increase performance</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0"><B>Prototype version 0.4.7 (Jan 27)</B></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Supports page background color and background image</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Supports text fields with text formats</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Supports images of JPEG, GIF, PNG formats</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Supports media of SWF format</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Supports internal Flash linkings</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Supports external HTTP linkings with new browser window</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Supports rectangles with different dimensions and colors</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) Navigation menu design improved with better proformance and security</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0"><B>Prototype version 0.3.8 (Jan 16)</B></FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Message boxes to show system messages</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (New) Message field supports icon and coloring styles</FONT></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#000000" LETTERSPACING="0" KERNING="0" /></P></TEXTFORMAT><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="10" COLOR="#666666" LETTERSPACING="0" KERNING="0">- (Fix) Scroll bar bug for extreme scrollings</FONT></P></TEXTFORMAT></text></TextFieldMC><TextFieldMC depth="4"><x>80</x><y>250</y><width>200</width><height>20</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="12" COLOR="#A6C104" LETTERSPACING="0" KERNING="0"><B>UPDATE HISTORY</B></FONT></P></TEXTFORMAT></text></TextFieldMC><RectangleMC depth="3"><x>40</x><y>240</y><width>520</width><height>260</height><rotation>0</rotation><corner>0</corner><line_style>1|0x999999|100</line_style><fill_color>0xffffff</fill_color><alpha>100</alpha></RectangleMC><TextFieldMC depth="2"><x>80</x><y>100</y><width>200</width><height>20</height><text><TEXTFORMAT LEADING="2"><P ALIGN="LEFT"><FONT FACE="Verdana" SIZE="12" COLOR="#A6C104" LETTERSPACING="0" KERNING="0"><B>WELCOME TO FLABER</B><FONT SIZE="10" /></FONT></P></TEXTFORMAT></text></TextFieldMC><RectangleMC depth="1"><x>40</x><y>90</y><width>520</width><height>135</height><rotation>0</rotation><corner>0</corner><line_style>1|0x999999|100</line_style><fill_color>0xffffff</fill_color><alpha>100</alpha></RectangleMC></data></PageContent>